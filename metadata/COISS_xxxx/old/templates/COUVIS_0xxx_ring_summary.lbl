PDS_VERSION_ID                  = PDS3
RECORD_TYPE                     = FIXED_LENGTH
FILE_RECORDS                    = $
RECORD_BYTES                    = 661
^RING_GEOMETRY_TABLE            = $

VOLUME_ID                       = $
PRODUCT_CREATION_TIME           = $

INSTRUMENT_HOST_NAME            = "CASSINI ORBITER"
INSTRUMENT_HOST_ID              = CO
INSTRUMENT_NAME                 = "ULTRAVIOLET IMAGING SPECTROGRAPH"
INSTRUMENT_ID                   = "UVIS"

OBJECT                          = RING_GEOMETRY_TABLE
  INTERCHANGE_FORMAT            = ASCII
  ROWS                          = $
  ROW_BYTES                     = 661
  COLUMNS                       = 59

  DESCRIPTION                   = "This index describes the ring intercept
    geometry of the field of view of Cassini UVIS observations. The table
    generally contains one row for each data file found on the volume
    identified by the VOLUME_ID keyword above.

    Occasionally, SPICE pointing information is unavailable for the time at
    which an observation was taken. When this situation occurs, no row has
    been added to the file.

    This geometry information has been constructed by sampling every UVIS
    spatial pixel in the associated data file. For each sampled pixel, a
    variety of geometric quantitities have been calculated, and the minimum
    and maximum values of each quantity have been tabulated. Note that, for
    angular quantities that cycle from 360 degrees to zero, the tabulated
    minimum can be numerically greater than the maximum. If the ring plane
    does not fall within the field of view, values of the NULL_CONSTANT appear
    in the table.

    Regions of the ring plane that are obscured by Saturn or by a targeted
    moon are excluded from the tabulation."

  OBJECT                        = COLUMN
    COLUMN_NUMBER               = 1
    NAME                        = VOLUME_ID
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 2
    BYTES                       = 11
    FORMAT                      = "A11"
    DESCRIPTION                 = "The volume ID provides a unique identifier
      for a PDS data volume."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    COLUMN_NUMBER               = 2
    NAME                        = FILE_SPECIFICATION_NAME
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 16
    BYTES                       = 40
    FORMAT                      = "A40"
    DESCRIPTION                 = "The file specification name provides the
      full name of a file, including a path name, relative to the root
      directory of the PDS volume."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    COLUMN_NUMBER               = 3
    NAME                        = RING_OBSERVATION_ID
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 59
    BYTES                       = 32
    FORMAT                      = "A32"
    DESCRIPTION                 = "The ring observation id uniquely identifies
      a single experiment or observation (image, occultation profile,
      spectrum, etc.) within a data set. This is the common id by which data
      are identified within the Rings Node catalog."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    COLUMN_NUMBER               = 4
    NAME                        = MINIMUM_RIGHT_ASCENSION
    DATA_TYPE                   = ASCII_REAL
    START_BYTE                  = 93
    BYTES                       = 10
    FORMAT                      = "F10.6"
    UNIT                        = "DEGREES"
    VALID_MINIMUM               = 0.
    VALID_MAXIMUM               = 360.
    NULL_CONSTANT               = -999.
    DESCRIPTION                 = "This is the minimum value for J2000 right
      ascension within the field of view sampled by this observation. For
      fields of view that cross 360 degrees, the tabulated minimum value
      will be greater than the maximum."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
