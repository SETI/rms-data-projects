    DATA_TYPE                   = ASCII_REAL
    START_BYTE                  = 207
    BYTES                       = 12
    FORMAT                      = "F12.3"
    UNIT                        = "KM"
    NULL_CONSTANT               = -999.
    DESCRIPTION                 = "Ring distance equals the separation between
