  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = "MAXIMUM_LONGITUDE_WRT_OBSERVER"
    UNIT                        = "DEGREES"
    VALID_MINIMUM               = -180.
    VALID_MAXIMUM               = 180.
    NULL_CONSTANT               = -999.
    DESCRIPTION                 = "This alternative definition of longitude
      is measured relative to the direction toward the observer. Values
      increase westward. Unlike other longitudes, this quantity ranges from
